import './src/database/database'

generator client {
  provider = "prisma-client-js"
}

generator zod {
  provider = "zod-prisma-types"
  output = "../src/zod/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

plugin trpc {
    provider = '@zenstackhq/trpc'
    output = 'src/trpc/generated'
    generateClientHelpers = 'react'
}
